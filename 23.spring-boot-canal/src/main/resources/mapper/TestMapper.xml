<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cmmplb.canal.dao.TestMapper">

    <sql id="base_column">
         t.`id`
        ,t.`id`
        ,t.`name`
        ,t.`create_time`
    </sql>

    <sql id="base_select">
        select
        <include refid="base_column"/>
        from `test` t
    </sql>

    <select id="selectByPaged" resultType="com.cmmplb.canal.vo.TestVO">
        <include refid="base_select"/>
        <where>
        </where>
        order by t.`id` desc
    </select>
</mapper>