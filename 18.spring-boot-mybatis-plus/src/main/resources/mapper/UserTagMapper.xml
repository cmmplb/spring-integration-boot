<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.cmmplb.mybatis.plus.dao.UserTagMapper">

    <sql id="base_column">
        <!-- @sql select -->
        ut.`id`,
        ut.`user_id`,
        ut.`tag_id`,
        ut.`create_time`,
        ut.`create_by`
        <!-- @sql from `user_tag` ut -->
    </sql>

    <sql id="base_select">
        select
        <include refid="base_column"/>
        from `user_tag` ut
    </sql>

    <select id="selectByUserId" resultType="io.github.cmmplb.mybatis.plus.entity.UserTag">
        <include refid="base_select"/>
        where ut.`user_id` = #{userId}
    </select>
</mapper>